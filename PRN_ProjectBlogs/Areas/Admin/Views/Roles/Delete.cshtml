@model PRN_ProjectBlogs.Models.Role

@{
    ViewData["Title"] = "XÓA TRUY CẬP";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>XÓA QUYỀN TRUY CẬP</h1>


<div class="slim-pageheader">
    <ol class="breadcrumb slim-breadcrumb">
        <li class="breadcrumb-item"><a asp-area="Admin" asp-controller="Home" asp-action="Index">Home</a></li>
        <li class="breadcrumb-item"><a asp-area="Admin" asp-controller="Roles" asp-action="Index">QUẢN LÝ TRUY CẬP</a></li>
        <li class="breadcrumb-item active" aria-current="page">XÓA QUYỀN TRUY CẬP</li>
    </ol>
    <h6 class="slim-pagetitle">XÓA TRUY CẬP</h6>
</div><!-- slim-pageheader -->
<div>
    <div class="section-wrapper mg-t-20">
        <div class="form-layout form-layout-7">
            <div class="row no-gutters">
                <div class="col-5 col-sm-4">
                    ID:
                </div><!-- col-4 -->
                <div class="col-7 col-sm-8">
                    <input asp-for="RoleId" class="form-control" type="text">
                </div><!-- col-8 -->
            </div><!-- row -->
            <div class="row no-gutters">
                <div class="col-5 col-sm-4">
                    Role Name:
                </div><!-- col-4 -->
                <div class="col-7 col-sm-8">
                    <input asp-for="RoleName" class="form-control" type="text">
                </div><!-- col-8 -->
            </div><!-- row -->
            <div class="row no-gutters">
                <div class="col-5 col-sm-4">
                    Role Description:
                </div><!-- col-4 -->
                <div class="col-7 col-sm-8">
                    <input asp-for="RoleDescription" class="form-control" type="text">
                </div><!-- col-8 -->
            </div><!-- row -->
        </div>
        <div style-"text-align:center !important" class-"form-layout-footer mg t-30">
            <form asp-area="Admin"  asp-controller="Roles" asp-route-id="@Model.RoleId">
                <input type ="hidden" asp-For="RoleId" />
                <input type= "submit" value="Xác nhận xóa" class="btn btn-danger" />
                <a asp-area ="Admin" asp-controller="Roles" asp-action="Index" class-"btn btn-success bd-0"> Back to List</a>
            </form>
        </div>
        </div>
</div>



           
          
